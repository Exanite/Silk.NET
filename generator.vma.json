{
    "tasks": [
        {
            "profileName": "VulkanMemoryAllocator",
            "sources": [
                "src/Vulkan/vk_vma.h"
            ],
            "mode": "Clang",
            "cacheDir": "build/cache",
            "cacheKey": "vulkan_vma",
            "controlDescriptors": [
                "no-pre-2.18-obsolete-enums"
            ],
            "converter": {},
            "clang": {
                "args": [
                    "--language=c++",
                    "--std=c++17",
                    "-m64",
                    "-Ibuild/submodules/VulkanMemoryAllocator/include",
                    "-Ibuild/submodules/Vulkan-Headers/include"
                ],
                "traverse": [],
                "classes": {
                    "vk_vma.h": "[Core]Vma"
                }
            },
            "exclude": [],
            "rename": {
                "VmaAllocation_T": "Allocation",
                "VmaAllocator_T": "Allocator",
                "VmaDefragmentationContext_T": "DefragmentationContext",
                "VmaPool_T": "Pool",
                "VmaVirtualAllocation_T": "VirtualAllocation",
                "VmaVirtualBlock_T": "VirtualBlock"
            },
            "bakery": {
                "profileNames": [
                    "vk_vma"
                ]
            },
            "output": {
                "mode": "Default",
                "path": "src/Vulkan",
                "licenseFile": "build/LICENSE_HEADER.txt",
                "props": "build/props/bindings.props"
            },
            "prefix": "Vma",
            "namespace": "Silk.NET.Vulkan.Vma",
            "extensionsNamespace": "Silk.NET.Vulkan.Vma.Extensions",
            "nameContainer": {
                "linux-x64": "libvma.so",
                "win-x64": "vma.dll",
                "win-x86": "vma.dll",
                "osx-x64": "libvma.dylib",
                "android": "libvma.so",
                "iOS": "libvma.dylib",
                "className": "VulkanMemoryAllocatorLibraryNameContainer"
            },
            "typeMaps": [
                {
                    "VkAllocationCallbacks": "Silk.NET.Vulkan.AllocationCallbacks",
                    "VkBindBufferMemoryInfo": "Silk.NET.Vulkan.BindBufferMemoryInfo",
                    "VkBindImageMemoryInfo": "Silk.NET.Vulkan.BindImageMemoryInfo",
                    "VkBufferCopy": "Silk.NET.Vulkan.BufferCopy",
                    "VkBufferCreateInfo": "Silk.NET.Vulkan.BufferCreateInfo",
                    "VkBufferMemoryRequirementsInfo2": "Silk.NET.Vulkan.BufferMemoryRequirementsInfo2",
                    "VkBuffer_T": "Silk.NET.Vulkan.Buffer",
                    "VkCommandBuffer_T": "Silk.NET.Vulkan.CommandBuffer",
                    "VkDeviceBufferMemoryRequirements": "Silk.NET.Vulkan.DeviceBufferMemoryRequirements",
                    "VkDeviceImageMemoryRequirements": "Silk.NET.Vulkan.DeviceImageMemoryRequirements",
                    "VkDeviceMemory_T": "Silk.NET.Vulkan.DeviceMemory",
                    "VkDevice_T": "Silk.NET.Vulkan.Device",
                    "VkImageCreateInfo": "Silk.NET.Vulkan.ImageCreateInfo",
                    "VkImageMemoryRequirementsInfo2": "Silk.NET.Vulkan.ImageMemoryRequirementsInfo2",
                    "VkImage_T": "Silk.NET.Vulkan.Image",
                    "VkInstance_T": "Silk.NET.Vulkan.Instance",
                    "VkMappedMemoryRange": "Silk.NET.Vulkan.MappedMemoryRange",
                    "VkMemoryAllocateInfo": "Silk.NET.Vulkan.MemoryAllocateInfo",
                    "VkMemoryRequirements": "Silk.NET.Vulkan.MemoryRequirements",
                    "VkMemoryRequirements2": "Silk.NET.Vulkan.MemoryRequirements2",
                    "VkPhysicalDeviceMemoryProperties": "Silk.NET.Vulkan.PhysicalDeviceMemoryProperties",
                    "VkPhysicalDeviceMemoryProperties2": "Silk.NET.Vulkan.PhysicalDeviceMemoryProperties2",
                    "VkPhysicalDeviceProperties": "Silk.NET.Vulkan.PhysicalDeviceProperties",
                    "VkPhysicalDevice_T": "Silk.NET.Vulkan.PhysicalDevice",
                    "VkResult": "Silk.NET.Vulkan.Result"
                },
                {
                    "$include.commonTypeMap": "build/csharp_typemap.json"
                }
            ]
        },
    ]
}
